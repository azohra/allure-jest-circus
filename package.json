{
  "name": "allure-jest-circus",
  "version": "0.1.1",
  "description": "aluure reporting for the jest-circus runner",
  "main": "index.js",
  "dependencies": {
    "allure-js-commons": "1.3.2",
    "jest": "^24.9.0",
    "jest-environment-node": "^24.9.0",
    "strip-ansi": "^5.2.0"
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.5",
    "semantic-release": "^15.13.24"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "author": "Jack Wilde",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azohra/allure-jest-circus.git"
  },
  "keywords": [
    "allure",
    "jest-circus",
    "jest"
  ],
  "bugs": {
    "url": "https://github.com/azohra/allure-jest-circus/issues"
  },
  "homepage": "https://github.com/azohra/allure-jest-circus#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
